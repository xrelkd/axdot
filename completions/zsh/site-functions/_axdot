#compdef axdot

autoload -U is-at-least

_axdot() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
":: :_axdot_commands" \
"*::: :->axdot" \
&& ret=0
    case $state in
    (axdot)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:axdot-command-$line[1]:"
        case $line[1] in
            (version)
_arguments "${_arguments_options[@]}" \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
&& ret=0
;;
(completions)
_arguments "${_arguments_options[@]}" \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
':shell:_files' \
&& ret=0
;;
(init)
_arguments "${_arguments_options[@]}" \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
&& ret=0
;;
(apply)
_arguments "${_arguments_options[@]}" \
'--config=[Configuration file path]' \
'-r[Replaces files/folders if they already exist]' \
'--replace[Replaces files/folders if they already exist]' \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
&& ret=0
;;
(dry-apply)
_arguments "${_arguments_options[@]}" \
'--config=[Configuration file path]' \
'-r[Replaces files/folders if they already exist]' \
'--replace[Replaces files/folders if they already exist]' \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" \
'-h[Prints help information]' \
'--help[Prints help information]' \
'-V[Prints version information]' \
'--version[Prints version information]' \
&& ret=0
;;
        esac
    ;;
esac
}

(( $+functions[_axdot_commands] )) ||
_axdot_commands() {
    local commands; commands=(
        "version:Shows current version" \
"completions:Shows shell completions" \
"init:Generates empty configuration" \
"apply:Applies from configuration file" \
"dry-apply:Shows what would be applied" \
"help:Prints this message or the help of the given subcommand(s)" \
    )
    _describe -t commands 'axdot commands' commands "$@"
}
(( $+functions[_axdot__apply_commands] )) ||
_axdot__apply_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot apply commands' commands "$@"
}
(( $+functions[_axdot__completions_commands] )) ||
_axdot__completions_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot completions commands' commands "$@"
}
(( $+functions[_axdot__dry-apply_commands] )) ||
_axdot__dry-apply_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot dry-apply commands' commands "$@"
}
(( $+functions[_axdot__help_commands] )) ||
_axdot__help_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot help commands' commands "$@"
}
(( $+functions[_axdot__init_commands] )) ||
_axdot__init_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot init commands' commands "$@"
}
(( $+functions[_axdot__version_commands] )) ||
_axdot__version_commands() {
    local commands; commands=(
        
    )
    _describe -t commands 'axdot version commands' commands "$@"
}

_axdot "$@"